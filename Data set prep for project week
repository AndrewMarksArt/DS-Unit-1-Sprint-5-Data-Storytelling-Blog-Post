      "source": [
        "country = []\n",
        "currency = auction_history['Currency']\n",
        "\n",
        "for i in range(len(currency)):\n",
        "  try:\n",
        "    country.append(country_dict.get(currency[i]))\n",
        "  except:\n",
        "    country.append(np.nan)\n",
        "    \n",
        "auction_history['Country'] = country"
