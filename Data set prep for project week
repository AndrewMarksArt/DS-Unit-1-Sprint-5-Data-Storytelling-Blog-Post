        "price = []\n",
        "\n",
        "for i in range(len(auction_history['Realized_Price'])):\n",
        "  try:\n",
        "    if 'Not' in auction_history['Realized_Price'][i]:\n",
        "      price.append(0)\n",
        "    elif 'not' in auction_history['Realized_Price'][i]:\n",
        "      price.append(0)\n",
        "    else:\n",
        "      price.append(auction_history['Realized_Price'][i])\n",
        "  except:\n",
        "    price.append(0)\n",
        "\n",
