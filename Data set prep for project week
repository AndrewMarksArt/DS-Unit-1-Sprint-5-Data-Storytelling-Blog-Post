      },
      "source": [
        "# use cleaning function to remove currency code from realized prices, lots that\n",
        "# were withdrawn get a -1 value\n",
        "\n",
        "auction_history['Realized_Price'] = clean_realized_price(auction_history['Realized_Price'])"
      ],
      "execution_count": 0,
      "outputs": []
